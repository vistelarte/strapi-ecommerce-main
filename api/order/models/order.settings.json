{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "payment": {
      "type": "enumeration",
      "enum": [
        "paid",
        "unpaid"
      ]
    },
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "delivered",
        "hold"
      ],
      "default": "pending"
    },
    "totalPrice": {
      "type": "decimal"
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "products": {
      "collection": "products",
      "via": "orders",
      "dominant": true
    }
  }
}
